{
  "name": "portal-graphql",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "build": "npm run generate:prisma && tsc -project tsconfig.json",
    "start": "doppler run -- node build/src/server.js",
    "dev": "doppler run -- npx ts-node-dev --inspect=9229 -r tsconfig-paths/register src/server.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:migrate": "doppler run -- npx prisma migrate dev",
    "db:reset": "doppler run -- npx prisma migrate reset",
    "db:seed": "doppler run -- npx ts-node prisma/seed.ts",
    "db:setup": "doppler run -- npm run db:migrate && npm run db:seed",
    "db:deploy": "doppler run -- npx prisma migrate deploy",
    "db:studio": "doppler run -- prisma studio",
    "generate:prisma": "npx prisma generate",
    "generate:graphql": "graphql-codegen --config codegen.yml",
    "generate:web": "cd ../web && npm run generate:web",
    "generate": "npm run generate:prisma && npm run generate:graphql && npm run generate:web",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.8",
    "@graphql-codegen/typescript-resolvers": "2.6.1",
    "@types/aws-lambda": "^8.10.95",
    "@types/lodash": "^4.14.181",
    "@types/node-fetch": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "2.6.2",
    "prisma": "^3.12.0",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.78.0",
    "@prisma/client": "^3.12.0",
    "apollo-server": "^3.6.7",
    "apollo-server-express": "^3.6.7",
    "aws-lambda": "^1.0.7",
    "date-fns": "^2.28.0",
    "graphql": "^16.3.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.7",
    "querystring": "^0.2.1",
    "tsconfig-paths": "^3.14.1",
    "uuid": "^8.3.2"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
